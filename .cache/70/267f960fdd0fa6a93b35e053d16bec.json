; }\n\n.sidebar__search {\n  margin: 1.5rem;\n  padding: 1rem;\n  width: 90%;\n  border: 1px solid #ceea20ec;\n  box-shadow: rgba(0, 0, 0, 0.1) 5px 3px 8px;\n  font-size: 1rem;\n  pointer-events: none; }\n\n::placeholder {\n  color: rgba(0, 0, 0, 0.432);\n  font-size: 1rem; }\n\n.sidebar__listings__item--click::after {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  content: ' '; }\n\n.sidebar__listings__item {\n  position: relative; }\n\n.sidebar__listings {\n  height: 80%;\n  overflow: auto;\n  padding-bottom: 10rem;\n  padding-left: 10px;\n  border-bottom: 2px solid black; }\n\n.sidebar__listings .sidebar__listings__item {\n  border-bottom: 1px solid #eee;\n  padding: 10px;\n  text-decoration: none; }\n\n.sidebar__listing .sidebar__listings__item:hover {\n  background-color: #ceea20ec; }\n\n.sidebar__listings .sidebar__listings__item:last-child {\n  border-bottom: none; }\n\n.sidebar__listings .sidebar__listings__item .sidebar__listings__item__title {\n  display: block;\n  color: #8b9115f3;\n  font-weight: 700; }\n\n.sidebar__listings\n.sidebar__listings__item\n.sidebar__listings__item__title\nsmall {\n  font-weight: 400; }\n\n.sidebar__listings\n.sidebar__listings__item.active\n.sidebar__listings__item__title,\n.sidebar__listings\n.sidebar__listings__item\n.sidebar__listings__item__title:hover {\n  color: #a3be15fe; }\n\n.sidebar__listings .sidebar__listings__item.active {\n  background-color: #f8f8f8; }\n\n.sidebar__listings .sidebar__listings__item:hover {\n  background-color: #ceea20ec;\n  transition: background-color 0.2s; }\n\n::-webkit-scrollbar {\n  width: 1rem;\n  height: 3px;\n  border-left: 0;\n  box-shadow: 0;\n  background: rgba(0, 0, 0, 0.1); }\n\n::-webkit-scrollbar-track {\n  background: none; }\n\n::-webkit-scrollbar-thumb {\n  background: #8b9115f3;\n  border-radius: 0; }\n\n* {\n  box-sizing: border-box; }\n\nbody {\n  color: #404040;\n  font: 400 15px/22px 'Source Sans Pro', 'Helvetica Neue', sans-serif;\n  margin: 